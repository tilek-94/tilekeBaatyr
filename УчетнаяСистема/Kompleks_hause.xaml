<UserControl x:Class="УчетнаяСистема.Kompleks_hause"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:УчетнаяСистема"
        mc:Ignorable="d"
         Loaded="Window_Loaded" Background="{x:Null}">

    <Grid >
        
        <Grid Background="White" Opacity="0.1" Grid.Row="1"></Grid>
        <ScrollViewer Grid.Row="2" Background="White" Margin="5,5,10,5">
            <WrapPanel x:Name="Panell"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></WrapPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
